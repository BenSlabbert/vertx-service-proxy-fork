{
  "name": "Proxy",
  "generators": [
    {
      "kind": "proxy",
      "fileName": "ifacePackageName.replace('io.vertx.ext', 'io.vertx.ext.proxy') + '.' + ifaceSimpleName + 'Proxy.java'",
      "templateFileName": "proxy-gen/template/proxygen.templ"
    },
    {
      "kind": "proxy",
      "fileName": "ifacePackageName.replace('io.vertx.ext', 'io.vertx.ext.proxy') + '.' + ifaceSimpleName + 'Handler.java'",
      "templateFileName": "proxy-gen/template/handlergen.templ"
    }
  ]
}
